{
  "schemaVersion": 1,
  "id": "skylens",
  "version": "${version}",
  "name": "Skylens",
  "description": "A Hypixel Skyblock mod for 1.21.11 that focuses on unique features and customizability.",
  "authors": [
    "next_g"
  ],
  "contributors": [
    "meowora",
    "j10a1n15",
    "zium1337",
    "MicrocontrollersDev",
    "superhize"
  ],
  "contact": {
    "sources": "https://github.com/nextrg/SkylensKotlin",
    "issues": "https://github.com/nextrg/SkylensKotlin/issues"
  },
  "license": "GPL v3",
  "icon": "assets/skylens/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "org.nextrg.skylens.Skylens"
    ],
    "modmenu": [
      "org.nextrg.skylens.ModConfig"
    ]
  },
  "mixins": [
    "skylens.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric-api": "*",
